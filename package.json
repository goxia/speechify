{
  "name": "speechify",
  "displayName": "Speechify",
  "description": "",
  "icon": "./icon.png",
  "version": "1.0.3",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "luckyXmobile",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.speechify",
        "title": "Speechify"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.speechify",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Speechify",
      "properties": {
        "speechify.azureSpeechServicesKey": {
          "type": "string",
          "default": "",
          "description": "Azure SpeechServices Key"
        },
        "speechify.speechServicesRegion": {
          "type": "string",
          "default": "eastus",
          "description": "SpeechServices Region"
        },
        "speechify.voiceName": {
          "type": "string",
          "default": "zh-CN-YunyangNeural",
          "description": "Voice Name"
        },
        "speechify.voiceGender": {
          "type": "string",
          "default": "Male",
          "description": "Voice Gender"
        },
        "speechify.voiceStyle": {
          "type": "string",
          "default": "friendly",
          "description": "Voice Style"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.5.0"
  }
}
